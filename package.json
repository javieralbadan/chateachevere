{
  "name": "chatea-chevere",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:vercel": "next build",
    "preexport:firebase": "rm -rf ./__api_temp__ && mv ./app/api ./__api_temp__ && rm -rf ./__admin_temp__ && mv ./app/admin ./__admin_temp__",
    "postexport:firebase": "mv ./__api_temp__ ./app/api && mv ./__admin_temp__ ./app/admin",
    "build:firebase": "npm run preexport:firebase && DEPLOY_TARGET=firebase next build",
    "deploy:firebase": "npm run build:firebase && firebase deploy --project chateachevere && npm run postexport:firebase",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@vercel/kv": "^3.0.0",
    "antd": "^5.20.3",
    "crypto": "^1.0.1",
    "firebase-admin": "^13.4.0",
    "luxon": "^3.6.1",
    "next": "^15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.5.0",
    "react-responsive-carousel": "^3.2.23",
    "react-string-replace": "^1.1.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.6.2",
    "@types/next": "^8.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  }
}
